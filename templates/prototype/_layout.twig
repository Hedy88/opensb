<body>
		<div class="top bg-light">
			<a href="../"><img class="picon" src="../assets/logo.png"/></a>
			<ul class="menu left">
				<li><a href="../" class="btn">{{ icon('circle', 16) }} Home</a></li>
				<li><a href="../browse.php" class="btn">{{ icon('circle', 16) }} Videos</a></li>
				<li><a href="../music.php" class="btn">{{ icon('circle', 16) }} Music</a></li>
				<li><a href="../artwork.php" class="btn">{{ icon('circle', 16) }}  Artwork</a></li>
				<li><a href="../forum/" class="btn">{{ icon('circle', 16) }} Forums</a></li>
			</ul>
			<ul class="menu right">
				<li><em>
				<a href="../search.php" class="btn">{{ icon('search', 16) }}</a>
					{% if log %}
						<a href="../upload.php">{{ icon('upload', 16) }}</a>
						{{ userlink(userdata) }}
						{% if notification_count > 0 %}
							<span class="notifs">[{{ notification_count }}]</span>
						{% endif %}
					{% else %}
						<a href="../login.php">Sign in</a>
						<a href="../register.php">Register</a>
					{% endif %}
				</em></li>
			</ul>
		</div>
	{% block precontent %}{% endblock %}
	<div class="container">
		{% block content %}{% endblock %}
	</div>
		<footer class="p-3">
		<div class="container">
		<div class="card">
		<div class="card-body">
		<h4 class="my-0">squareBracket Next</h4>
		<p class="my-0"><b>{{ __("By the squareBracket contributors") }}</b> Â· 2020-{{ 'now' | date('Y') }} | <b>{{ __("Users registered") }}</b>: {{ stats.usercount }} | <b>{{ __("Videos uploaded") }}</b>: {{ stats.videocount }}</p>
		</div>
		</div>
	</footer>
</body>
</html>