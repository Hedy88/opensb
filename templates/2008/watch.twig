} 


function showhide_morefrom() {
  var x = document.getElementById("watch-more-from");
  var xs = document.getElementById("arrow_morefrom");
  if (x.style.display === "block") {
    x.style.display = "none";
	xs.setAttribute('src', 'img/arrow2.png');
  } else {
    x.style.display = "block";
	xs.setAttribute('src', 'img/arrow1.png');
  }
}

function showhide_related() {
  var y = document.getElementById("watch_related");
  var ys = document.getElementById("arrow_related");
  if (y.style.display === "block") {
    y.style.display = "none";
	ys.setAttribute('src', 'img/arrow2.png');
  } else {
    y.style.display = "block";
	ys.setAttribute('src', 'img/arrow1.png');
  }
}
function show_ratingnli() {
	var ratingmessage = document.getElementById("defaultRatingMessage");
	var hoverMessage = document.getElementById("hoverMessage");
    ratingmessage.style.display = "none";
	hoverMessage.style.display = "block";
  }
function hide_ratingnli() {
	var ratingmessage = document.getElementById("defaultRatingMessage");
	var hoverMessage = document.getElementById("hoverMessage");
    ratingmessage.style.display = "block";
	hoverMessage.style.display = "none";
  }
                      function favorite_video(el) {
                        if (el.innerHTML == "Add to Favorites") {
                            el.innerHTML = "Remove from Favorites";
                        } else {
                            el.innerHTML = "Add to Favorites";
                        }
                    }
                    function flag_video(el) {
                        if (el.innerHTML == "Flag This Video") {
                            el.innerHTML = "Remove Flag";
